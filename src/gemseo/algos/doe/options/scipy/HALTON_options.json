{
  "name": "HALTON_options",
  "required": [
    "n_samples"
  ],
  "properties": {
    "eval_jac": {
      "type": "boolean"
    },
    "n_processes": {
      "type": "integer",
      "minimum": 1
    },
    "n_samples": {
      "type": "integer",
      "minimum": 1
    },
    "max_time": {
      "type": "number",
      "minimum": 0.0
    },
    "seed": {
      "anyOf": [
        {
          "type": "null"
        },
        {
          "type": "integer",
          "minimum": 0
        }
      ]
    },
    "wait_time_between_samples": {
      "type": "number",
      "minimum": 0
    },
    "scramble": {
      "type": "boolean",
      "default": true
    },
    "optimization": {
      "anyOf": [
        {
          "type": "null"
        },
        {
          "type": "string",
          "enum": [
            "random-cd",
            "lloyd"
          ]
        }
      ]
    }
  },
  "$schema": "http://json-schema.org/draft-04/schema",
  "additionalProperties": false,
  "type": "object"
}
